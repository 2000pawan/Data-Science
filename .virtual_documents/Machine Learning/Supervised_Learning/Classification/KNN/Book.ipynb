newdata=[[4,2,1,1,1,2,3,2,1],[8,10,10,8,7,10,9,7,1]]


prediction=model.predict(newdata)
for i in prediction:
    if i==2:
        print('It is starting stage of Cancer. Cure itüôåüèº')
